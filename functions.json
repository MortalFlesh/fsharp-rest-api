{
  "bindings": [
    {
      "authLevel": "anonymous",
      "name": "req",
      "type": "httpTrigger",
      "direction": "in",
      "route": "{filename}/{name}"
    },
    {
      "name": "res",
      "type": "http",
      "direction": "out"
    },
    {
      "type": "blob",
      "name": "template",
      "path": "html/{filename}.html",
      "direction": "in"
    }
  ],
  "disabled": false
}
